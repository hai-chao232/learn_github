        <mxCell id="c2CH0Qo7M9obde4qKBL8-29" value="&lt;font style=&quot;font-size: 15px;&quot;&gt;本地仓库连接GitHub仓库&lt;/font&gt;" style="rounded=1;whiteSpace=wrap;html=1;" parent="1" vertex="1">
